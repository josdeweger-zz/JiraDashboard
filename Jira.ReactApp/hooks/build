#!/bin/sh
docker build \
    -f jira-react-app.dockerfile \
    --build-arg REACT_APP_TEAM_NAME=$REACT_APP_TEAM_NAME \
    --build-arg REACT_APP_JIRA_NODE_SERVER_SETTINGS_URL=$REACT_APP_JIRA_NODE_SERVER_SETTINGS_URL \
    --build-arg REACT_APP_JIRA_API_CUSTOMER_STATUS_URL=$REACT_APP_JIRA_API_CUSTOMER_STATUS_URL \
    -t $IMAGE_NAME .